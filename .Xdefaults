URxvt*cursorBlink:     true
URxvt*cursorUnderline: false 
URxvt*font:            xft:Ubuntu Mono:size=14
URxvt*boldFont:        xft:Ubuntu Mono:size=14:bold
URxvt*scrollBar:       false
URxvt.perl-ext-common: default,matcher,url-select,tabbed
URxvt.url-launcher:    firefox
URxvt.underlineURLs:   true
URxvt.matcher.button:  1
URxvt.internalBorder:  16
URxvt.lineSpace:       1
URxvt.letterSpace:     -1

URxvt.perl-lib: /home/danila/.urxvt/perl

URxvt*saveLines: 5000

URxvt.tabbed.new-button: no
URxvt.tabbed.tabbar-fg: 15
URxvt.tabbed.tabbar-bg: 8
URxvt.tabbed.tab-fg:    8
URxvt.tabbed.tab-bg:    14

